{
  "algorithm": {
    "parameters": [
      {
        "name": "args",
        "type": "ArgsListUsers"
      }
    ],
    "statements": [
      {
        "command": "Return",
        "expression": "RdsSelect(MyDataBase.TblUsers, This(), [RdsConditional(id == ^args.id, !IsNull(^args.id)), RdsConditional(externalId == ^args.externalId, !IsNull(^args.externalId)), RdsConditional(userName == ^args.userName, !IsNull(^args.userName))], userName, ^args.offset, ^args.limit)"
      }
    ],
    "typeReturn": "list(User)",
    "variables": []
  },
  "routing": {
    "method": "GET",
    "parameters": [
      {
        "bind": "args.id",
        "in": "Query",
        "name": "id",
        "remarks": "For filtering."
      },
      {
        "bind": "args.externalId",
        "in": "Query",
        "name": "externalId",
        "remarks": "For filtering."
      },
      {
        "bind": "args.userName",
        "in": "Query",
        "name": "userName",
        "remarks": "For filtering."
      },
      {
        "bind": "args.offset",
        "in": "Query",
        "name": "offset",
        "remarks": "Requested index for start the results to be provided in response."
      },
      {
        "bind": "args.limit",
        "in": "Query",
        "name": "limit",
        "remarks": "Requested number results to be provided in response."
      }
    ],
    "path": "/users",
    "remarks": "Allows to get a list of Users.",
    "responses": []
  }
}